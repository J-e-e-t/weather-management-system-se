<div>
    <div>
        <div id="carouselSlider" class="carousel slide">
            <ol class="carousel-indicators">
                <li data-target="#carouselSlider" data-slide-to="0" class="active"></li>
                <li data-target="#carouselSlider" data-slide-to="1"></li>
                <li data-target="#carouselSlider" data-slide-to="2"></li>
            </ol>
            <div class="carousel-inner">
                <div class="carousel-item active">
                    <img src="./CC1.jpg" class="w-100 h-100 ">
                    <div class="carousel-caption ">
                        <h1>CC 1</h1>
                        <p>Temp: {{data.cc1_temp}} Degree</p>
                        <p>Humidity: {{data.cc1_humidity}}%</p>
                        <p>Feels Like: {{data.cc1_feels_like}}</p>
                        <p>Max Temp: {{data.cc1_max_temp}}</p>
                        <p>Min Temp: {{data.cc1_min_temp}}</p>
                    </div>
                </div>
                <div class="carousel-item ">
                    <img src="./CC2.jpg" class="w-100 h-100 ">
                    <div class="carousel-caption ">
                        <h1>CC 2</h1>
                        <p>Temp: {{data.cc2_temp}} Degree</p>
                        <p>Humidity: {{data.cc2_humidity}}%</p>
                        <p>Feels Like: {{data.cc2_feels_like}}</p>
                        <p>Max Temp: {{data.cc2_max_temp}}</p>
                        <p>Min Temp: {{data.cc2_min_temp}}</p>
                    </div>
                </div>
                <div class="carousel-item ">
                    <img src="./CC3.jpg" class="w-100 h-100 ">
                    <div class="carousel-caption ">
                        <h1>CC 3</h1>
                        <p>Temp: {{data.cc3_temp}} Degree</p>
                        <p>Humidity: {{data.cc3_humidity}}%</p>
                        <p>Feels Like: {{data.cc3_feels_like}}</p>
                        <p>Max Temp: {{data.cc3_max_temp}}</p>
                        <p>Min Temp: {{data.cc3_min_temp}}%</p>
                    </div>
                </div>
            </div>
            <a class="carousel-control-prev " href="#carouselSlider" role="button" data-slide="prev">
                <span class="carousel-control-prev-icon "></span>
                <span>Previous</span>
            </a>
            <a class="carousel-control-next " href="#carouselSlider" role="button" data-slide="next">
                <span class="carousel-control-next-icon "></span>
                <span>Next</span>
            </a>
        </div>
    </div>
    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js "
